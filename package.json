{
  "name": "@suyongs/solid-utility",
  "version": "0.4.1",
  "description": "Utilities for SolidJS",
  "main": "dist/index.umd.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "homepage": "https://github.com/Su-Yong/solid-utility",
  "bugs": "https://github.com/Su-Yong/solid-utility/issues",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "pnpm clean && pnpm build:only",
    "build:only": "vite build",
    "dev": "vite build --watch",
    "example:marquee": "cd examples/marquee && pnpm dev",
    "example:transition": "cd examples/transition && pnpm dev",
    "release": "dotenv release-it"
  },
  "keywords": [
    "solid",
    "solidjs",
    "marquee",
    "transition"
  ],
  "author": "Su-Yong",
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/node": "20.11.28",
    "dotenv-cli": "7.4.1",
    "release-it": "17.1.1",
    "rimraf": "5.0.5",
    "terser": "5.29.2",
    "typescript": "5.4.2",
    "vite": "5.1.6",
    "vite-plugin-dts": "3.7.3",
    "vite-plugin-solid": "2.10.2"
  },
  "peerDependencies": {
    "solid-js": "*"
  },
  "dependencies": {
    "nanoid": "5.0.6"
  }
}
